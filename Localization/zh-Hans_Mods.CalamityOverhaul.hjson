		EnableSwordLight: {
			Label: 启用刀光
			Tooltip:
				'''
				默认开启，许多刀剑类武器将会拥有炫彩亮丽的刀光
				在绝大多数情况下，这会让游戏变得更酷
				'''
		}

		MuraUIStyleType: {
			Label: 鬼妖村正UI样式选择
